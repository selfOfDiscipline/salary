<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyzq.salary.mapper.SalaryPersonTaxMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tyzq.salary.model.SalaryPersonTax">
        <id column="id" property="id" />
        <result column="tax_name" property="taxName" />
        <result column="start_money" property="startMoney" />
        <result column="end_money" property="endMoney" />
        <result column="tax" property="tax" />
        <result column="deduct_money" property="deductMoney" />
        <result column="max_tax_flag" property="maxTaxFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tax_name, start_money, end_money, tax, deduct_money, max_tax_flag
    </sql>

</mapper>
