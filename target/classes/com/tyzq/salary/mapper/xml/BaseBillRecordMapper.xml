<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyzq.salary.mapper.BaseBillRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tyzq.salary.model.BaseBillRecord">
        <id column="id" property="id" />
        <result column="en_code" property="enCode" />
        <result column="bill_type" property="billType" />
        <result column="bill_code" property="billCode" />
        <result column="bill_prefix" property="billPrefix" />
        <result column="bill_suffix" property="billSuffix" />
        <result column="sort_num" property="sortNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, en_code, bill_type, bill_code, bill_prefix, bill_suffix, sort_num
    </sql>

</mapper>
